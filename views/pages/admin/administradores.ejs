<div class="row">
  <div class="col-md-12">
    <% flash.get('mensaje').forEach(function (msg) { %>
    <div class="alert alert-info text-center" role="alert"><%=msg%></div>
    <% }) %>

    <h2>Lista de Cliente</h2>

    <table class="table">
      <thead>
        <tr>
          <th scope="col">id</th>
          <th scope="col">Nombre</th>
          <th scope="col">Acciones</th>
        </tr>
      </thead>
      <tbody>
        <% admin.forEach( admin => { %>
        <tr>
          <td scope="row"><%=admin.id%></td>
          <td><%=admin.nombre%></td>

          <td>
            <% if(admin.email == adminSessions.email){ %>
            <a href="" class="btn btn-info" style="visibility: hidden"
              >No se Puede desactivar</a
            >
            <%}else{ if (admin.activa) { %>
            <a href="/admin/desactivar-admin/<%=admin.id%>" class="btn btn-info"
              >Desactivar Admin</a
            >
            <% } else { %>
            <a href="/admin/activar-admin/<%=admin.id%>" class="btn btn-primary"
              >Activar Admin</a
            >
            <% }} %>
          </td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </div>
</div>
