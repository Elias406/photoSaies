<div class="row">
  <div class="col-md-12">
    <% flash.get('mensaje').forEach(function (msg) { %>
    <div class="alert alert-success" role="alert"><%=msg%></div>
    <% }) %>

    <h2>Fotos de La Orden</h2>

    <table class="table">
      <thead>
        <tr>
          <th>id</th>
          <th>titulo</th>
          <th>foto</th>
        </tr>
      </thead>
      <tbody>
        <% fotos.forEach( foto => { %>
        <tr>
          <td><%=foto.id%></td>
          <td><%=foto.titulo%></td>
          <td>
            <img src="/images/fotos/<%=foto.contenido%>" alt="" class="wf-10" />
          </td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </div>
</div>
